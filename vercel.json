{
  "redirects": [
    {
      "source": "/",
      "has": [{ "type": "host", "value": "app.misionantigua.org" }],
      "destination": "/app",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "app.misionantigua.org" }],
      "destination": "/app/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
